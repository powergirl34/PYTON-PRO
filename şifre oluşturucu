import random

karakterler = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
şifre_uzunluğu = int(input("Parolanız kaç karakter uzunluğunda olsun? "))
oluşturulan_parola = ""
for _ in range(şifre_uzunluğu):
    oluşturulan_parola += random.choice(karakterler)
print("Oluşturulan parola:", oluşturulan_parola)

